{
  "expo": {
    "name": "myfirstapp",
    "slug": "myfirstapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "https://c0.wallpaperflare.com/preview/379/493/189/concept-development-device-flat.jpg",
      "resizeMode": "cover",
      "backgroundColor": "light"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
